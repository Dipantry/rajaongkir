{
  "name": "dipantry/rajaongkir",
  "description": "RajaOngkir API wrapper for PHP 8.0+",
  "keywords": [
    "cek-ongkir",
    "rajaongkir",
    "api",
    "wrapper",
    "php",
    "laravel"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0.0|^10.0.0",
    "flynsarmy/csv-seeder": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "php-coveralls/php-coveralls": "^2.1",
    "orchestra/testbench": "^7.0",
    "ext-openssl": "*"
  },
  "authors": [
    {
      "role": "Developer",
      "name": "Auriga Aristo",
      "email": "aurigaaristo@gmail.com",
      "homepage": "https://aurigaaristo.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Dipantry\\Rajaongkir\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dipantry\\Rajaongkir\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}