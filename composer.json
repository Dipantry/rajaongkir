{
  "name": "dipantry/rajaongkir",
  "description": "RajaOngkir API wrapper for PHP 8.0+",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "php-coveralls/php-coveralls": "^2.1",
    "orchestra/testbench": "^7.0"
  },
  "authors": [
    {
      "role": "Developer",
      "name": "Auriga Aristo",
      "email": "aurigaaristo@gmail.com",
      "homepage": "https://aurigaaristo.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Dipantry\\RajaOngkir\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dipantry\\RajaOngkir\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}